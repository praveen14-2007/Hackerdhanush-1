#include <stdio.h>
int main() {
    char studentID[] = "CS2025A9";
    char deptCode[3];
    int start = 0;    
    int length = 2;   
    for (int i = 0; i < length; i++) {
        deptCode[i] = studentID[start + i];
    }
    deptCode[length] = '\0';  
    printf("Department Code: %s\n", deptCode);
    return 0;
}
